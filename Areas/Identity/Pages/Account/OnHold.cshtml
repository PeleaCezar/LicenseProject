@page
@model LicenseProject.Areas.Identity.Pages.Account.OnHoldModel

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewData["Title"] - LicenseProject</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Theme/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Theme/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div style="background-color:white">
                <div class="align-content-lg-stretch">
                    <div class="card-header">
                        <h1 class="card-title lead">
                           Detalii despre inregistrare
                        </h1>
                    </div>
                    <div class="card-body" style="height:750px">
                        
                            <h6>
                                Salut,<strong> @Model.Email </strong>! Pana in acest moment cererea ta nu a fost preluata de catre un administrator .
                            </h6>
                            
                            <h6>Te rugam sa revii mai tarziu .</h6>
                        

                    </div>
                </div>
            </div>
            </div>
        </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Theme/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Theme/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Theme/dist/js/adminlte.min.js"></script>


</body>
</html>
